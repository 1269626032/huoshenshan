<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>


    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://39.105.43.28:3306/question?useUnicode=true"
                        userId="nouyuchen"
                        password="Nouyuchen@)!&amp;youngmall2019">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>


        <javaModelGenerator targetPackage="com.wuhan2020.email_admin.entity"
                            targetProject="/Users/noudepc/IdeaProjects/question_admin/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--生成mapper目录-->
        <sqlMapGenerator targetPackage="mapping"
                         targetProject="/Users/noudepc/IdeaProjects/question_admin/src/main/resources">
            <property name="enableSubPackages" value="true"/>
